{
	"name": "bAdBlocker",
	"version": "1.0.0",
	"manifest_version": 3,
	"description": "Chrome URL Blocking Extension",
	"permissions": ["declarativeNetRequest"],
	"declarative_net_request": {
		"rule_resources": [
			{
				"id": "ruleset_1",
				"enabled": true,
				"path": "rules.json"
			}
		]
	}
}
